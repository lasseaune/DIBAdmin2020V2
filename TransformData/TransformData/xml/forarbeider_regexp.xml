        </part>
        <part type="optional">
          <part type="regexp" name="parantesfront"/>
        </part>
        <part type="regexp" name="year"/>
        <part type="optional">
          <part type="regexp" name="parantesrear"/>
        </part>
      </part>
    </expression>
  </regexp>

