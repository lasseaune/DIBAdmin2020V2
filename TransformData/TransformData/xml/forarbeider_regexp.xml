  <regexp type="single">
    <id>kapnumber</id>
    <description>avsnitt IV.1.1.1</description>
    <expression>
      <part type="named" name="kapnum">
        <part type="single">
          <text><![CDATA[\d+]]></text>
        </part>
        <part type="optional">
          <part type="multiple">
            <part type="single">
              <text><![CDATA[\.\d+]]></text>
            </part>
          </part>
        </part>
      </part>
    </expression>
  </regexp>

