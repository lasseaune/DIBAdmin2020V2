          <part type="single">
            <text><![CDATA[(i|I)(n|N)+(s|S)(t|T)]]></text>
          </part>
        </part>
        <part type="optional">
          <text><![CDATA[(\s+)?(\.)?(\s+)?]]></text>
        </part>
        <part type="optional">
          <part type="regexp" name="nr"/>
        </part>
        <part type="chain">
          <part type="negative lookahead">
            <part type="single">
              <text><![CDATA[0(\.|\s)]]></text>
            </part>
          </part>
          <part type="named" name="number">
            <part type="single">
              <text><![CDATA[\d+]]></text>
            </part>
          </part>
        </part>
        <part type="optional">
          <text><![CDATA[(\s+)?(\.)?(\s+)?]]></text>
        </part>
        <part type="optional">
          <part type="regexp" name="vedtaktype" />
